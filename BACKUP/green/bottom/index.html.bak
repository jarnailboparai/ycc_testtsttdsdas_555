<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>Coffe Shop</title>
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="../../assets/css/fonts.css" />
<link rel="stylesheet" href="css/jquery.mobile-1.4.0.css" />
<link rel="stylesheet" href="css/wooden_theme.css" />
<link rel="stylesheet" href="css/common_layout.css" />
<script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
<script src="js/jquery.mobile-1.4.0.js"></script>
<!-- Mobile Crousel Setting -->
<link rel="stylesheet" href="css/mobile_crousel/mobile_crousel.css" />
<link rel="stylesheet" href="../common/css/custom_responsive.css" />
<!-- Mobile Crousel  Settings ends Here -->
<link rel="stylesheet" href="css/temporary.css" />
<style type="text/css">
img {
	max-width:100%;
}
</style>
</head>

<body>
<!-- Start of first page -->
<div data-role="page" id="theme_layout" class="theme_bg"> 
  
  <!-- Header Starts Here -->
  
  <!-- Main Content Starts  Here -->
  <div role="main" class="ui-content"> 
    <!-- Logo of app -->
    <div class="logo" align="center"><img src="../common/images/logo.png" alt="logo" /></div>
  </div>
  <!-- /content ends here --> 
  
  <!-- Footer Starts Here -->
  <div data-role="footer" data-position="fixed" > 
    <!-- Menu Starts Here -->
    <div class="owl-carousel owl-demo" >
      <div class="app_feature_list"> <a class="item link" rel="external" data-ajax="false" href="../common/social.html"><img src="images/fb.png" /></a>
      <a rel="external" data-ajax="false" href="../common/social.html">  <label>Add Facebook</label></a>
      </div>
      <div class="app_feature_list"><a class="item link" rel="external" data-ajax="false" href="../common/location.html"><img src="images/add_location.png" /></a>
       <a rel="external" data-ajax="false" href="../common/location.html"> <label>Add Location</label></a>
      </div>
      <div class="app_feature_list"> <a class="item link" rel="external" data-ajax="false" href="../common/video_gallery.html"><img src="images/youtube.png" /></a>
        <a rel="external" data-ajax="false" href="../common/video_gallery.html"><label>Video<br />
          Gallery</label></a>
      </div>
      <div class="app_feature_list"> <a class="item link" rel="external" data-ajax="false" href="../common/splash.html"><img src="images/add_phone.png" /></a>
       <a rel="external" data-ajax="false" href="../common/splash.html"> <label>Splash<br />
          Screen</label></a>
      </div>
     <div class="app_feature_list"> <a class="item link" rel="external" data-ajax="false" href="../common/static_page_list.html"><img src="images/multiple_page.png" /></a>
      <a rel="external" data-ajax="false" href="../common/static_page_list.html">  <label>Static Page</label></a>
      </div>
      <div class="app_feature_list"> <a class="item link" rel="external" data-ajax="false" href="../common/picture_gallery.html"><img src="images/photo_gallery.png" /></a>
        <a rel="external" data-ajax="false" href="../common/picture_gallery.html"><label>Photo<br />
			Gallery</label></a>
      </div>
      <div class="app_feature_list"> <a class="item link" rel="external" data-ajax="false" href="../common/contact_us.html"><img src="images/contact_us_page_icon.png" /></a>
        <a rel="external" data-ajax="false" href="../common/contact_us.html"><label>Contact Us</label></a>
      </div>
      
      
      
      
    </div>
    
    <!-- Menu ends Here --> 
  </div>
  <!-- footer ends here --> 
</div>
<!-- /page --> 
<script src="js/mobile_crousel_js/mobile_crousel.js"></script> 
<script>
    $(document).ready(function() {
     
    var owl = $(".owl-demo");
     
    owl.owlCarousel({
    itemsCustom : [
    [0, 3],
    [450, 4],
    [600, 5],
    [700, 6],
    [1000, 8],
    [1200, 10],
    [1400, 13],
    [1600, 15]
    ],
    navigation : false
     
    });
     
    });

</script>
